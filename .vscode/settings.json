{
  "[properties]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true
  },
  "css.format.enable": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "always"
  },
  "editor.defaultFoldingRangeProvider": "esbenp.prettier-vscode",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "eslint.codeActionsOnSave.mode": "all",
  "eslint.enable": true,
  "eslint.format.enable": false,
  "eslint.probe": ["javascript", "javascriptreact", "typescript", "typescriptreact"],
  "eslint.run": "onType",
  "eslint.validate": ["javascript", "javascriptreact", "typescript", "typescriptreact"],
  "explorer.confirmDragAndDrop": false,
  "files.exclude": {
    "**/node_modules/**": true,
    ".git": true
  },
  "files.watcherExclude": {
    "**/dist/**": true,
    "**/node_modules/**": true,
    ".git": true
  },
  "html.format.enable": false,
  "javascript.format.enable": false,
  "javascript.suggestionActions.enabled": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "javascript.validate.enable": false,
  "markdown.occurrencesHighlight.enabled": true,
  "material-icon-theme.activeIconPack": "react",
  "material-icon-theme.files.color": "#000000",
  "material-icon-theme.folders.color": "#eb7734",
  "material-icon-theme.hidesExplorerArrows": true,
  "npm.exclude": "release/**",
  "search.exclude": {
    "**/dist/**": true,
    ".git": true,
    "node_modules": true,
    "npm-debug.log.*": true,
    "package-lock.json": true
  },
  "template-string-converter.autoRemoveTemplateString": true,
  "template-string-converter.convertOutermostQuotes": true,
  "template-string-converter.validLanguages": [
    "javascript",
    "typescriptreact",
    "javascriptreact",
    "typescript"
  ],
  "terminal.integrated.automationProfile.windows": {
    "color": "terminal.ansiGreen",
    "icon": "terminal-linux",
    "overrideName": true,
    "path": "${env:PROGRAMFILES}\\Git\\bin\\bash.exe",
    "env": {
      "TARGET_BROWSER": "chrome",
      "PATH": "${env:PROGRAMFILES}\\Google\\Chrome\\Application;${env:PATH}"
    }
  },
  "terminal.integrated.defaultProfile.windows": "node-bash",
  "terminal.integrated.profiles.windows": {
    "node-bash": {
      "color": "terminal.ansiCyan",
      "env": {
        "PATH": "${workspaceFolder}\\node_modules\\.bin:${env:PATH}",
        "TARGET_BROWSER": "chrome"
      },
      "icon": "terminal-bash",
      "overrideName": true,
      "path": "${env:PROGRAMFILES}\\Git\\bin\\bash.exe"
    }
  },
  "typescript.format.enable": false,
  "typescript.tsdk": "node_modules\\typescript\\lib",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "workbench.iconTheme": "material-icon-theme"
}
